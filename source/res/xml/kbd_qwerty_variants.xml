<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:horizontalGap="0px"
    android:keyHeight="@dimen/key_height"
    android:keyWidth="10%p"
    android:verticalGap="0px" >

    <Row android:rowEdgeFlags="top" >
        <Key
            android:codes="113"
            android:keyEdgeFlags="left"
            android:keyLabel="q" />
        <Key
            android:codes="119"
            android:keyLabel="w" />
        <Key
            android:codes="101"
            android:keyLabel="e"
            android:popupCharacters="@string/alternates_for_e"
            android:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            android:codes="114"
            android:keyLabel="r"
            android:popupCharacters="@string/alternates_for_r"
            android:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            android:codes="116"
            android:keyLabel="t"
            android:popupCharacters="@string/alternates_for_t"
            android:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            android:codes="121"
            android:keyLabel="y"
            android:popupCharacters="@string/alternates_for_y"
            android:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            android:codes="117"
            android:keyLabel="u"
            android:popupCharacters="@string/alternates_for_u"
            android:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            android:keyLabel="@string/key_i"
            android:popupCharacters="@string/alternates_for_i"
            android:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            android:codes="111"
            android:keyLabel="o"
            android:popupCharacters="@string/alternates_for_o"
            android:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            android:codes="112"
            android:keyLabel="p" />
        <Key
            android:codes="-7"
            android:keyEdgeFlags="right"
            android:keyLabel=" "
            android:keyWidth="0%p" />
    </Row>
    <Row>
        <Key
            android:codes="97"
            android:keyEdgeFlags="left"
            android:keyLabel="a"
            android:popupCharacters="@string/alternates_for_a"
            android:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            android:codes="115"
            android:keyLabel="s"
            android:popupCharacters="@string/alternates_for_s"
            android:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            android:codes="100"
            android:keyLabel="d"
            android:popupCharacters="@string/alternates_for_d"
            android:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            android:codes="102"
            android:keyLabel="f" />
        <Key
            android:codes="103"
            android:keyLabel="g"
            android:popupCharacters="@string/alternates_for_g"
            android:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            android:codes="104"
            android:keyLabel="h" />
        <Key
            android:codes="106"
            android:keyLabel="j" />
        <Key
            android:codes="107"
            android:keyLabel="k" />
        <Key
            android:codes="108"
            android:keyLabel="l"
            android:popupCharacters="@string/alternates_for_l"
            android:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            android:codes="@integer/key_variants"
            android:isModifier="true"
            android:isSticky="true"
            android:keyLabel="áž" />
        <Key
            android:codes="-7"
            android:keyEdgeFlags="right"
            android:keyLabel=" "
            android:keyWidth="0%p" />
    </Row>
    <Row>
        <Key
            android:codes="-1"
            android:iconPreview="@drawable/sym_keyboard_feedback_shift"
            android:isModifier="true"
            android:isSticky="true"
            android:keyEdgeFlags="left"
            android:keyIcon="@drawable/sym_keyboard_shift"
            android:keyWidth="15%p" />
        <Key
            android:codes="122"
            android:keyLabel="z"
            android:popupCharacters="@string/alternates_for_z"
            android:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            android:codes="120"
            android:keyLabel="x" />
        <Key
            android:codes="99"
            android:keyLabel="c"
            android:popupCharacters="@string/alternates_for_c"
            android:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            android:codes="118"
            android:keyLabel="v" />
        <Key
            android:codes="98"
            android:keyLabel="b" />
        <Key
            android:codes="110"
            android:keyLabel="n"
            android:popupCharacters="@string/alternates_for_n"
            android:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            android:codes="109"
            android:keyLabel="m" />
        <Key
            android:codes="-5"
            android:iconPreview="@drawable/sym_keyboard_feedback_delete"
            android:isRepeatable="true"
            android:keyIcon="@drawable/sym_keyboard_delete"
            android:keyWidth="15%p" />
        <Key
            android:codes="-7"
            android:keyEdgeFlags="right"
            android:keyLabel=" "
            android:keyWidth="0%p" />
    </Row>
    <Row
        android:keyboardMode="@+id/mode_normal"
        android:rowEdgeFlags="bottom" >
        <Key
            android:codes="-4"
            android:iconPreview="@drawable/sym_keyboard_feedback_done"
            android:keyEdgeFlags="left"
            android:keyIcon="@drawable/sym_keyboard_done"
            android:keyWidth="15%p" />
        <Key
            android:codes="-2"
            android:keyLabel="@string/label_symbol_key"
            android:keyWidth="20%p"
            android:popupCharacters="_"
            android:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            android:keyLabel=","
            android:keyWidth="10%p" />
        <Key
            android:codes="32"
            android:iconPreview="@drawable/sym_keyboard_feedback_space"
            android:isRepeatable="true"
            android:keyIcon="@drawable/sym_keyboard_space"
            android:keyWidth="25%p" />
        <Key
            android:codes="46"
            android:keyLabel="."
            android:keyWidth="10%p"
            android:popupKeyboard="@xml/popup_punctuation" />
        <Key
            android:codes="10"
            android:iconPreview="@drawable/sym_keyboard_feedback_return"
            android:keyIcon="@drawable/sym_keyboard_return"
            android:keyWidth="20%p" />
        <Key
            android:codes="-7"
            android:keyEdgeFlags="right"
            android:keyLabel=" "
            android:keyWidth="0%p" />
    </Row>
    <Row
        android:keyboardMode="@+id/mode_url"
        android:rowEdgeFlags="bottom" >
        <Key
            android:codes="-4"
            android:iconPreview="@drawable/sym_keyboard_feedback_done"
            android:keyEdgeFlags="left"
            android:keyIcon="@drawable/sym_keyboard_done"
            android:keyWidth="15%p" />
        <Key
            android:codes="-2"
            android:keyLabel="@string/label_symbol_key"
            android:keyWidth="20%p"
            android:popupCharacters="_"
            android:popupKeyboard="@xml/kbd_popup_template" />
        <!--
             <Key android:keyLabel="@string/popular_domain_0"
                android:keyOutputText="@string/popular_domain_0"
                android:popupKeyboard="@xml/popup_domains"
                android:keyWidth="15%p"/>
        -->
        <Key
            android:keyLabel="/"
            android:keyWidth="15%p" />
        <Key
            android:codes="32"
            android:iconPreview="@drawable/sym_keyboard_feedback_space"
            android:isRepeatable="true"
            android:keyIcon="@drawable/sym_keyboard_space"
            android:keyWidth="15%p" />
        <Key
            android:keyLabel="."
            android:keyWidth="15%p"
            android:popupKeyboard="@xml/popup_punctuation" />
        <Key
            android:codes="10"
            android:iconPreview="@drawable/sym_keyboard_feedback_return"
            android:keyIcon="@drawable/sym_keyboard_return"
            android:keyWidth="20%p" />
        <Key
            android:codes="-7"
            android:keyEdgeFlags="right"
            android:keyLabel=" "
            android:keyWidth="0%p" />
    </Row>
    <Row
        android:keyboardMode="@+id/mode_email"
        android:rowEdgeFlags="bottom" >
        <Key
            android:codes="-4"
            android:iconPreview="@drawable/sym_keyboard_feedback_done"
            android:keyEdgeFlags="left"
            android:keyIcon="@drawable/sym_keyboard_done"
            android:keyWidth="15%p" />
        <Key
            android:codes="-2"
            android:keyLabel="@string/label_symbol_key"
            android:keyWidth="20%p"
            android:popupCharacters="_"
            android:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            android:keyLabel="\@"
            android:keyWidth="15%p" />
        <!--
             <Key android:keyLabel="@string/popular_domain_0"
                android:keyOutputText="@string/popular_domain_0"
                android:popupKeyboard="@xml/popup_domains"
                android:keyWidth="15%p"/>
        -->
        <Key
            android:codes="32"
            android:iconPreview="@drawable/sym_keyboard_feedback_space"
            android:isRepeatable="true"
            android:keyIcon="@drawable/sym_keyboard_space"
            android:keyWidth="15%p" />
        <Key
            android:keyLabel="."
            android:keyWidth="15%p"
            android:popupKeyboard="@xml/popup_punctuation" />
        <Key
            android:codes="10"
            android:iconPreview="@drawable/sym_keyboard_feedback_return"
            android:keyIcon="@drawable/sym_keyboard_return"
            android:keyWidth="20%p" />
        <Key
            android:codes="-7"
            android:keyEdgeFlags="right"
            android:keyLabel=" "
            android:keyWidth="0%p" />
    </Row>
    <Row
        android:keyboardMode="@+id/mode_im"
        android:rowEdgeFlags="bottom" >
        <Key
            android:codes="-4"
            android:iconPreview="@drawable/sym_keyboard_feedback_done"
            android:keyEdgeFlags="left"
            android:keyIcon="@drawable/sym_keyboard_done"
            android:keyWidth="15%p" />
        <Key
            android:codes="-2"
            android:keyLabel="@string/label_symbol_key"
            android:keyWidth="20%p"
            android:popupCharacters="_"
            android:popupKeyboard="@xml/kbd_popup_template"
            android:state_single="true" />
        <Key
            android:keyLabel=","
            android:keyWidth="10%p" />
        <Key
            android:codes="32"
            android:iconPreview="@drawable/sym_keyboard_feedback_space"
            android:isRepeatable="true"
            android:keyIcon="@drawable/sym_keyboard_space"
            android:keyWidth="25%p" />
        <Key
            android:codes="46"
            android:keyLabel="."
            android:keyWidth="10%p"
            android:popupKeyboard="@xml/popup_punctuation" />
        <Key
            android:keyLabel=":-)"
            android:keyOutputText=":-) "
            android:keyWidth="20%p"
            android:popupKeyboard="@xml/popup_smileys" />
        <Key
            android:codes="-7"
            android:keyEdgeFlags="right"
            android:keyLabel=" "
            android:keyWidth="0%p" />
    </Row>

</Keyboard>