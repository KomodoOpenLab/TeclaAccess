<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:horizontalGap="0px"
    android:keyHeight="@dimen/key_height"
    android:keyWidth="10%p"
    android:verticalGap="0px" >

    <Row
        android:keyboardMode="@+id/mode_normal"
        android:rowEdgeFlags="bottom" >
        <Key
            android:codes="-4"
            android:iconPreview="@drawable/sym_keyboard_feedback_show"
            android:keyEdgeFlags="left"
            android:keyIcon="@drawable/sym_keyboard_show"
            android:keyWidth="13%p" />
        <Key
            android:codes="4"
            android:iconPreview="@drawable/nav_keyboard_feedback_back"
            android:keyIcon="@drawable/nav_keyboard_back"
            android:keyWidth="13%p" />
        <Key
            android:codes="19"
            android:iconPreview="@drawable/nav_keyboard_feedback_up"
            android:keyIcon="@drawable/nav_keyboard_up"
            android:keyWidth="12%p" />
        <Key
            android:codes="20"
            android:iconPreview="@drawable/nav_keyboard_feedback_down"
            android:keyIcon="@drawable/nav_keyboard_down"
            android:keyWidth="12%p" />
        <Key
            android:codes="21"
            android:iconPreview="@drawable/nav_keyboard_feedback_left"
            android:keyIcon="@drawable/nav_keyboard_left"
            android:keyWidth="12%p" />
        <Key
            android:codes="22"
            android:iconPreview="@drawable/nav_keyboard_feedback_right"
            android:keyIcon="@drawable/nav_keyboard_right"
            android:keyWidth="12%p" />
        <Key
            android:codes="23"
            android:keyLabel="OK"
            android:keyWidth="13%p" />
        <Key
            android:codes="@integer/key_repeat_lock"
            android:iconPreview="@drawable/nav_keyboard_feedback_repeat"
            android:keyIcon="@drawable/nav_keyboard_repeat"
            android:keyEdgeFlags="right"
            android:keyWidth="13%p" />
    </Row>
    <Row
        android:keyboardMode="@+id/mode_voiceold"
        android:rowEdgeFlags="bottom" >
        <Key
            android:codes="@integer/key_done"
            android:iconPreview="@drawable/sym_keyboard_feedback_show"
            android:keyIcon="@drawable/sym_keyboard_show"
            android:keyEdgeFlags="left"
            android:keyWidth="12%p" />
        <Key
            android:codes="@integer/key_back"
            android:iconPreview="@drawable/nav_keyboard_feedback_back"
            android:keyIcon="@drawable/nav_keyboard_back"
            android:keyWidth="11%p" />
        <Key
            android:codes="@integer/key_dpad_up"
            android:iconPreview="@drawable/nav_keyboard_feedback_up"
            android:keyIcon="@drawable/nav_keyboard_up"
            android:keyWidth="11%p" />
        <Key
            android:codes="@integer/key_dpad_down"
            android:iconPreview="@drawable/nav_keyboard_feedback_down"
            android:keyIcon="@drawable/nav_keyboard_down"
            android:keyWidth="11%p" />
        <Key
            android:codes="@integer/key_dpad_left"
            android:iconPreview="@drawable/nav_keyboard_feedback_left"
            android:keyIcon="@drawable/nav_keyboard_left"
            android:keyWidth="11%p" />
        <Key
            android:codes="@integer/key_dpad_right"
            android:iconPreview="@drawable/nav_keyboard_feedback_right"
            android:keyIcon="@drawable/nav_keyboard_right"
            android:keyWidth="11%p" />
        <Key
            android:codes="@integer/key_dpad_center"
            android:keyLabel="OK"
            android:keyWidth="11%p" />
        <Key
            android:codes="@integer/key_voice"
            android:iconPreview="@drawable/sym_keyboard_feedback_mic"
            android:keyIcon="@drawable/sym_keyboard_mic"
            android:keyWidth="11%p" />
        <Key
            android:codes="-8"
            android:iconPreview="@drawable/nav_keyboard_feedback_repeat"
            android:keyIcon="@drawable/nav_keyboard_repeat"
            android:keyEdgeFlags="right"
            android:keyWidth="11%p" />
        </Row>
        <Row
        android:keyboardMode="@+id/mode_secnav_voice">
        <Key
            android:codes="@integer/key_voice"
            android:iconPreview="@drawable/sym_keyboard_feedback_mic"
            android:keyIcon="@drawable/sym_keyboard_mic"
            android:keyWidth="25%p" />
        <Key
            android:codes="-8"
            android:iconPreview="@drawable/nav_keyboard_feedback_repeat"
            android:keyIcon="@drawable/nav_keyboard_repeat"
            android:keyWidth="25%p"/>
        <Key
            android:codes="@integer/hide_secnav_kbd"
            android:keyLabel="Send to PC"
            android:keyEdgeFlags="right"
            android:keyWidth="50%p" />
        </Row>
    <Row
        android:keyboardMode="@+id/mode_secnav_voice"
        android:rowEdgeFlags="bottom" >
        <Key
            android:codes="@integer/key_done"
            android:iconPreview="@drawable/sym_keyboard_feedback_show"
            android:keyIcon="@drawable/sym_keyboard_show"
            android:keyEdgeFlags="left"
            android:keyWidth="12%p" />
        <Key
            android:codes="@integer/key_back"
            android:iconPreview="@drawable/nav_keyboard_feedback_back"
            android:keyIcon="@drawable/nav_keyboard_back"
            android:keyWidth="11%p" />
        <Key
            android:codes="@integer/key_dpad_up"
            android:iconPreview="@drawable/nav_keyboard_feedback_up"
            android:keyIcon="@drawable/nav_keyboard_up"
            android:keyWidth="11%p" />
        <Key
            android:codes="@integer/key_dpad_down"
            android:iconPreview="@drawable/nav_keyboard_feedback_down"
            android:keyIcon="@drawable/nav_keyboard_down"
            android:keyWidth="11%p" />
        <Key
            android:codes="@integer/key_dpad_left"
            android:iconPreview="@drawable/nav_keyboard_feedback_left"
            android:keyIcon="@drawable/nav_keyboard_left"
            android:keyWidth="11%p" />
        <Key
            android:codes="@integer/key_dpad_right"
            android:iconPreview="@drawable/nav_keyboard_feedback_right"
            android:keyIcon="@drawable/nav_keyboard_right"
            android:keyWidth="11%p" />
        <Key
            android:codes="@integer/key_dpad_center"
            android:keyLabel="OK"
            android:keyWidth="11%p" /> 
        <Key
            android:codes="@integer/hide_secnav_kbd"
            android:keyLabel="Less"
            android:keyEdgeFlags="right"
            android:keyWidth="23%p" /> 
        </Row>
        <Row
        android:keyboardMode="@+id/mode_voice"
        android:rowEdgeFlags="bottom" >
        <Key
            android:codes="@integer/key_done"
            android:iconPreview="@drawable/sym_keyboard_feedback_show"
            android:keyIcon="@drawable/sym_keyboard_show"
            android:keyEdgeFlags="left"
            android:keyWidth="12%p" />
        <Key
            android:codes="@integer/key_back"
            android:iconPreview="@drawable/nav_keyboard_feedback_back"
            android:keyIcon="@drawable/nav_keyboard_back"
            android:keyWidth="11%p" />
        <Key
            android:codes="@integer/key_dpad_up"
            android:iconPreview="@drawable/nav_keyboard_feedback_up"
            android:keyIcon="@drawable/nav_keyboard_up"
            android:keyWidth="11%p" />
        <Key
            android:codes="@integer/key_dpad_down"
            android:iconPreview="@drawable/nav_keyboard_feedback_down"
            android:keyIcon="@drawable/nav_keyboard_down"
            android:keyWidth="11%p" />
        <Key
            android:codes="@integer/key_dpad_left"
            android:iconPreview="@drawable/nav_keyboard_feedback_left"
            android:keyIcon="@drawable/nav_keyboard_left"
            android:keyWidth="11%p" />
        <Key
            android:codes="@integer/key_dpad_right"
            android:iconPreview="@drawable/nav_keyboard_feedback_right"
            android:keyIcon="@drawable/nav_keyboard_right"
            android:keyWidth="11%p" />
        <Key
            android:codes="@integer/key_dpad_center"
            android:keyLabel="OK"
            android:keyWidth="11%p" /> 
        <Key
            android:codes="@integer/show_secnav_kbd"
            android:keyLabel="More"
            android:keyEdgeFlags="right"
            android:keyWidth="23%p" /> 
        </Row>
    
    
</Keyboard>