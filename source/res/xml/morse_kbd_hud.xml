<Keyboard 
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:horizontalGap="0px"
        android:verticalGap="0px">
        
        <Row
            android:rowEdgeFlags="top"
            android:keyWidth="100%"
            android:keyHeight="30%"/>
        
        <Row
            android:keyWidth="13%"
            android:rowEdgeFlags="bottom"
            android:keyHeight="11%">
            
            <Key
                android:codes="-4"
                android:keyIcon="@drawable/sym_keyboard_done"
                android:iconPreview="@drawable/sym_keyboard_feedback_done"
                android:keyWidth="12%"
                android:keyEdgeFlags="left"/>
            <Key
                android:codes="500"
                android:keyLabel="â€¢"
                android:keyWidth="18%"
                android:isRepeatable="true"/>
            <Key
                android:codes="562"
                android:keyIcon="@drawable/sym_keyboard_space"
                android:keyWidth="26%"/>
            <Key
                android:codes="501"
                android:keyLabel="-"
                android:keyWidth="18%"
                android:isRepeatable="true"/>
            <Key
                android:codes="-1"
                android:iconPreview="@drawable/sym_keyboard_feedback_shift"
                android:keyIcon="@drawable/sym_keyboard_shift"
                android:isModifier="true"
                android:isSticky="true"/>
            <Key
                android:codes="67"
                android:keyIcon="@drawable/sym_keyboard_delete"
                android:isRepeatable="true"
                android:keyEdgeFlags="right"/>
        </Row>
</Keyboard>
