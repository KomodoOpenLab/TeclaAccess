<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:horizontalGap="0px"
    android:keyHeight="@dimen/key_height"
    android:keyWidth="10%p"
    android:verticalGap="0px" >

    <Row>
        <Key
            android:keyEdgeFlags="left"
            android:keyLabel="a"
            android:popupCharacters="@string/alternates_for_a"
            android:popupKeyboard="@xml/kbd_popup_template" />
        <Key android:keyLabel="b" />
        <Key
            android:keyLabel="c"
            android:popupCharacters="@string/alternates_for_c"
            android:popupKeyboard="@xml/kbd_popup_template" />
        <Key android:keyLabel="d" />
        <Key
            android:keyLabel="e"
            android:popupCharacters="@string/alternates_for_e"
            android:popupKeyboard="@xml/kbd_popup_template" />
        <Key android:keyLabel="f" />
        <Key android:keyLabel="g" />
        <Key android:keyLabel="h" />
        <Key
            android:keyLabel="i"
            android:popupCharacters="@string/alternates_for_i"
            android:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            android:keyEdgeFlags="right"
            android:keyLabel="j" />
        <Key
            android:codes="-7"
            android:keyLabel=" "
            android:keyWidth="0%p" />
    </Row>
    <Row android:keyboardMode="@+id/mode_normal" >
        <Key
            android:horizontalGap="5%p"
            android:keyEdgeFlags="left"
            android:keyLabel="k" />
        <Key android:keyLabel="l" />
        <Key android:keyLabel="m" />
        <Key
            android:keyLabel="n"
            android:popupCharacters="@string/alternates_for_n"
            android:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            android:keyLabel="o"
            android:popupCharacters="@string/alternates_for_o"
            android:popupKeyboard="@xml/kbd_popup_template" />
        <Key android:keyLabel="p" />
        <Key android:keyLabel="q" />
        <Key android:keyLabel="r" />
        <Key
            android:keyLabel="s"
            android:popupCharacters="@string/alternates_for_s"
            android:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            android:codes="-7"
            android:keyEdgeFlags="right"
            android:keyLabel=" "
            android:keyWidth="0%p" />
    </Row>
    <Row android:keyboardMode="@+id/mode_voice" >
        <Key
            android:horizontalGap="5%p"
            android:keyEdgeFlags="left"
            android:keyLabel="k" />
        <Key android:keyLabel="l" />
        <Key android:keyLabel="m" />
        <Key
            android:keyLabel="n"
            android:popupCharacters="@string/alternates_for_n"
            android:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            android:keyLabel="o"
            android:popupCharacters="@string/alternates_for_o"
            android:popupKeyboard="@xml/kbd_popup_template" />
        <Key android:keyLabel="p" />
        <Key android:keyLabel="q" />
        <Key android:keyLabel="r" />
        <Key
            android:keyLabel="s"
            android:popupCharacters="@string/alternates_for_s"
            android:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            android:codes="-7"
            android:keyEdgeFlags="right"
            android:keyLabel=" "
            android:keyWidth="0%p" />
    </Row>
    <Row android:keyboardMode="@+id/mode_variants" >
        <Key
            android:keyEdgeFlags="left"
            android:keyLabel="k" />
        <Key android:keyLabel="l" />
        <Key android:keyLabel="m" />
        <Key
            android:keyLabel="n"
            android:popupCharacters="@string/alternates_for_n"
            android:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            android:keyLabel="o"
            android:popupCharacters="@string/alternates_for_o"
            android:popupKeyboard="@xml/kbd_popup_template" />
        <Key android:keyLabel="p" />
        <Key android:keyLabel="q" />
        <Key android:keyLabel="r" />
        <Key
            android:keyLabel="s"
            android:popupCharacters="@string/alternates_for_s"
            android:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            android:codes="@integer/key_variants"
            android:isModifier="true"
            android:isSticky="true"
            android:keyLabel="áž" />
        <Key
            android:codes="-7"
            android:keyEdgeFlags="right"
            android:keyLabel=" "
            android:keyWidth="0%p" />
    </Row>
    <Row>
        <Key
            android:codes="-1"
            android:iconPreview="@drawable/sym_keyboard_feedback_shift"
            android:isModifier="true"
            android:isSticky="true"
            android:keyEdgeFlags="left"
            android:keyIcon="@drawable/sym_keyboard_shift"
            android:keyWidth="15%p" />
        <Key android:keyLabel="t" />
        <Key
            android:keyLabel="u"
            android:popupCharacters="@string/alternates_for_u"
            android:popupKeyboard="@xml/kbd_popup_template" />
        <Key android:keyLabel="v" />
        <Key android:keyLabel="w" />
        <Key android:keyLabel="x" />
        <Key
            android:keyLabel="y"
            android:popupCharacters="@string/alternates_for_y"
            android:popupKeyboard="@xml/kbd_popup_template" />
        <Key android:keyLabel="z" />
        <Key
            android:codes="-5"
            android:iconPreview="@drawable/sym_keyboard_feedback_delete"
            android:isRepeatable="true"
            android:keyIcon="@drawable/sym_keyboard_delete"
            android:keyWidth="15%p" />
        <Key
            android:codes="-7"
            android:keyEdgeFlags="right"
            android:keyLabel=" "
            android:keyWidth="0%p" />
    </Row>
    <Row
        android:keyboardMode="@+id/mode_normal"
        android:rowEdgeFlags="bottom" >
        <Key
            android:codes="-4"
            android:iconPreview="@drawable/sym_keyboard_feedback_done"
            android:keyEdgeFlags="left"
            android:keyIcon="@drawable/sym_keyboard_done"
            android:keyWidth="15%p" />
        <Key
            android:codes="-2"
            android:keyLabel="@string/label_symbol_key"
            android:keyWidth="20%p" />
        <Key
            android:keyLabel=","
            android:keyWidth="10%p" />
        <Key
            android:codes="32"
            android:iconPreview="@drawable/sym_keyboard_feedback_space"
            android:isRepeatable="true"
            android:keyIcon="@drawable/sym_keyboard_space"
            android:keyWidth="25%p" />
        <Key
            android:codes="46"
            android:keyLabel="."
            android:keyWidth="10%p"
            android:popupKeyboard="@xml/popup_punctuation" />
        <Key
            android:codes="10"
            android:iconPreview="@drawable/sym_keyboard_feedback_return"
            android:keyIcon="@drawable/sym_keyboard_return"
            android:keyWidth="20%p" />
        <Key
            android:codes="-7"
            android:keyEdgeFlags="right"
            android:keyLabel=" "
            android:keyWidth="0%p" />
    </Row>
    <Row
        android:keyboardMode="@+id/mode_voice"
        android:rowEdgeFlags="bottom" >
        <Key
            android:codes="-4"
            android:iconPreview="@drawable/sym_keyboard_feedback_done"
            android:keyEdgeFlags="left"
            android:keyIcon="@drawable/sym_keyboard_done"
            android:keyWidth="14%p" />
        <Key
            android:codes="-2"
            android:keyLabel="@string/label_symbol_key"
            android:keyWidth="16%p" />
        <Key
            android:keyLabel=","
            android:keyWidth="10%p" />
        <Key
            android:codes="32"
            android:iconPreview="@drawable/sym_keyboard_feedback_space"
            android:isRepeatable="true"
            android:keyIcon="@drawable/sym_keyboard_space"
            android:keyWidth="21%p" />
        <Key
            android:codes="46"
            android:keyLabel="."
            android:keyWidth="10%p"
            android:popupKeyboard="@xml/popup_punctuation" />
        <Key
            android:codes="10"
            android:iconPreview="@drawable/sym_keyboard_feedback_return"
            android:keyIcon="@drawable/sym_keyboard_return"
            android:keyWidth="16%p" />
        <Key
            android:codes="@integer/key_voice"
            android:iconPreview="@drawable/sym_keyboard_feedback_mic"
            android:keyIcon="@drawable/sym_keyboard_mic"
            android:keyWidth="13%p" />
        <Key
            android:codes="-7"
            android:keyEdgeFlags="right"
            android:keyLabel=" "
            android:keyWidth="0%p" />
    </Row>
    <Row
        android:keyboardMode="@+id/mode_variants"
        android:rowEdgeFlags="bottom" >
        <Key
            android:codes="-4"
            android:iconPreview="@drawable/sym_keyboard_feedback_done"
            android:keyEdgeFlags="left"
            android:keyIcon="@drawable/sym_keyboard_done"
            android:keyWidth="15%p" />
        <Key
            android:codes="-2"
            android:keyLabel="@string/label_symbol_key"
            android:keyWidth="20%p" />
        <Key
            android:keyLabel=","
            android:keyWidth="10%p" />
        <Key
            android:codes="32"
            android:iconPreview="@drawable/sym_keyboard_feedback_space"
            android:isRepeatable="true"
            android:keyIcon="@drawable/sym_keyboard_space"
            android:keyWidth="25%p" />
        <Key
            android:codes="46"
            android:keyLabel="."
            android:keyWidth="10%p"
            android:popupKeyboard="@xml/popup_punctuation" />
        <Key
            android:codes="10"
            android:iconPreview="@drawable/sym_keyboard_feedback_return"
            android:keyIcon="@drawable/sym_keyboard_return"
            android:keyWidth="20%p" />
        <Key
            android:codes="-7"
            android:keyEdgeFlags="right"
            android:keyLabel=" "
            android:keyWidth="0%p" />
    </Row>

</Keyboard>