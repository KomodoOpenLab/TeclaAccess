<Keyboard 
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:horizontalGap="0px"
        android:verticalGap="0px" 
        >

        
        <Row
                android:keyWidth="100%"
                android:rowEdgeFlags="top"
                android:keyHeight="30%"/>
        
        <Row
                android:keyWidth="13%"
                android:rowEdgeFlags="bottom"
                android:keyHeight="11%">
                
                <Key
                        android:keyLabel="@string/caps_lock_off"
                        android:keyEdgeFlags="left"
                        android:codes="559"
                        android:isSticky="true"/>     
                <Key
                        android:keyLabel="â€¢"
                        android:keyWidth="18%"
                        android:codes="0"/> 
                <Key
                        android:keyLabel="space"
                        android:keyWidth="26%"
                        android:codes="562"/>
                <Key
                        android:keyLabel="-"
                        android:keyWidth="18%"
                        android:codes="1"/>
                <Key
                        android:keyIcon="@drawable/sym_keyboard_delete"
                        android:isRepeatable="true"
                        android:codes="67"/>
                <Key 	
                    	android:codes="-4"
                    	android:keyIcon="@drawable/sym_keyboard_done"
                    	android:iconPreview="@drawable/sym_keyboard_feedback_done"
                    	android:keyWidth="12%"
                    	android:keyEdgeFlags="right"/>
        </Row>

</Keyboard>
    
