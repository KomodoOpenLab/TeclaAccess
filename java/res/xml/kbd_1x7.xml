<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:horizontalGap="0px"
    android:keyHeight="@dimen/key_height"
    android:keyWidth="14%p"
    android:verticalGap="0px" >

    <Row android:rowEdgeFlags="bottom" >
        <Key
            android:codes="@integer/key_done"
            android:iconPreview="@drawable/sym_keyboard_feedback_show"
            android:keyEdgeFlags="left"
            android:keyIcon="@drawable/sym_keyboard_show" />
        <Key
            android:codes="@integer/key_custom"
            android:horizontalGap="2%p"
            android:keyLabel="" />
        <Key
            android:codes="@integer/key_custom"
            android:keyLabel="" />
        <Key
            android:codes="@integer/key_custom"
            android:keyLabel="" />
        <Key
            android:codes="@integer/key_custom"
            android:keyLabel="" />
        <Key
            android:codes="@integer/key_custom"
            android:keyLabel="" />
        <Key
            android:codes="@integer/key_custom"
            android:keyEdgeFlags="right"
            android:keyLabel="" />
    </Row>

</Keyboard>