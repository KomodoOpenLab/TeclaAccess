<Keyboard 
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:horizontalGap="0px"
    android:verticalGap="0px">
    
    <Row
        android:rowEdgeFlags="top"
        android:keyHeight="29%">
    </Row>
        
    <Row
        android:keyWidth="13%"
        android:rowEdgeFlags="bottom"
        android:keyHeight="11%">
        
        <Key
            android:codes="@integer/key_done"
            android:keyIcon="@drawable/sym_keyboard_done"
            android:iconPreview="@drawable/sym_keyboard_feedback_done"
            android:keyWidth="14%"
            android:keyEdgeFlags="left"/>
        <Key
            android:codes="@integer/key_dit"
            android:keyLabel="â€¢"
            android:keyWidth="18%"
            android:isRepeatable="true"/>
        <Key
            android:codes="@integer/key_endof_char"
            android:keyIcon="@drawable/sym_keyboard_space"
            android:keyWidth="24%"/>
        <Key
            android:codes="@integer/key_dah"
            android:keyLabel="-"
            android:keyWidth="18%"
            android:isRepeatable="true"/>
        <Key
            android:codes="@integer/key_shift"
            android:iconPreview="@drawable/sym_keyboard_feedback_shift"
            android:keyIcon="@drawable/sym_keyboard_shift"
            android:isModifier="true"
            android:isSticky="true"/>
        <Key
            android:codes="67"
            android:keyIcon="@drawable/sym_keyboard_delete"
            android:isRepeatable="true"
            android:keyEdgeFlags="right"/>
    </Row>
</Keyboard>
